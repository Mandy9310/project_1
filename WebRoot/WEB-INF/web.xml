  </servlet-mapping>

	<servlet>
    <servlet-name>DisplayChart</servlet-name>
    <servlet-class>
        org.jfree.chart.servlet.DisplayChart   <!--这个固定不变-->
    </servlet-class>
    </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Servlet_unit</servlet-name>
    <servlet-class>lk.ysu.workingschedule.servlet.Servlet_unit</servlet-class>
  </servlet>

    
    <servlet-mapping>
        <servlet-name>DisplayChart</servlet-name>
        <url-pattern>/DisplayChart</url-pattern>
    </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_unit</servlet-name>
    <url-pattern>/servlet/Servlet_unit</url-pattern>
  </servlet-mapping>
