  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_system</servlet-name>
    <url-pattern>/servlet/Servlet_system</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_messageList</servlet-name>
    <url-pattern>/servlet/Servlet_messageList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_plan</servlet-name>
