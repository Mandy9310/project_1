    <servlet-name>Servlet_plan</servlet-name>
    <servlet-class>lk.ysu.workingschedule.servlet.Servlet_plan</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>Servlet_machine</servlet-name>
